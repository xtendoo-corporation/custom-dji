<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Comercial Juan Jose -->
        <record id="jjose_payments" model="res.groups">
            <field name="name">Juan Jose</field>
        </record>

         <record id="payment_term_jjose" model="ir.rule">
            <field name="name">Modos de pago Juan Jose</field>
            <field name="model_id" ref="account.model_account_journal"/>
            <field name="domain_force">['|',('name','=','Efectivo Juan Jose'),('type','in',('sale','purchase','general'))]</field>
            <field name="groups" eval="[(4, ref('especialidades_commercials_security.jjose_payments'))]"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_unlink" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_read" eval="True"/>
        </record>
        <!-- Fin Comercial Juan Jose -->
        <!-- Comercial Juan Jose -->
